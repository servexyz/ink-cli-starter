{
  "name": "cli-starter",
  "version": "0.5.0",
  "main": "build/cli.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/servexyz/cli-starter"
  },
  "bin": {
    "cstar": "build/cli.js"
  },
  "files": [
    "build/cli.js"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.39",
    "@babel/preset-env": "^7.0.0-beta.39",
    "apollo-boost": "^0.1.4",
    "ava": "^1.0.0-beta.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "chmod": "^0.2.1",
    "trevor": "^2.3.0",
    "webdash": "^1.2.0",
    "webdash-npm-scripts": "^1.2.0",
    "webdash-package-json": "^1.2.0"
  },
  "scripts": {
    "test": "ava",
    "start": "npm run build && npm run simple-start",
    "simple-start": "./build/cli.js unicorns --rainbow",
    "dev": "./cli.js unicorns --rainbow",
    "clean": "rm -rf build && mkdir build",
    "build-cli": "babel -d ./build ./cli.js",
    "build": "npm run clean && npm run build-cli",
    "ci:local": "trevor",
    "webd": "npx webdash serve"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "9.3.0"
          }
        }
      ]
    ],
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ]
  },
  "dependencies": {
    "chalk": "^2.3.1",
    "ink": "^0.4.1",
    "ink-router": "^0.6.0",
    "meow": "^4.0.0"
  }
}
